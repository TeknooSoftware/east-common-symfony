        "doctrine/mongodb-odm": "^2.15.3",
        "doctrine/mongodb-odm-bundle": "^5.5.1",
        "doctrine/persistence": "^4.1.1",
        "laminas/laminas-diactoros": "^3.8",
        "league/flysystem": "^3.30.2",
        "matthiasmullie/minify": "^1.3.75",
        "psr/http-message":"^2",
        "psr/http-factory":"^1.1",
        "symfony/form": "^6.4.24||^7.3||^8",
        "symfony/http-kernel": "^6.4.24||^7.3||^8",
        "symfony/notifier": "^6.4.24||^7.3||^8",
        "symfony/psr-http-message-bridge": "^6.4.24||^7.3||^8",
        "symfony/security-core": "^6.4.24||^7.3||^8",
        "symfony/security-bundle": "^6.4.24||^7.3||^8",
        "symfony/validator": "^6.4.24||^7.3||^8",
        "symfony/yaml": "^6.4.24||^7.3||^8",
        "teknoo/bridge-phpdi-symfony": "^7.1.2",
        "teknoo/immutable": "^3.0.22",
        "teknoo/states": "^7.1.2",
        "teknoo/recipe": "^7.2.2",
        "teknoo/east-foundation-symfony": "^9.0.4",
        "teknoo/east-common": "^4.1.4",
        "twig/twig": "^3.22.2"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "extra": {
        "symfony" : {
            "endpoint": [
                "https://api.github.com/repos/TeknooSoftware/symfony-recipes/contents/index.json",
                "flex://defaults"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "teknoo/composer-install": true
        }
    }
}
